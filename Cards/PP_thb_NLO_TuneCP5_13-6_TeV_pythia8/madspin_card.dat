set max_weight_ps_point 400
set Nevents_for_max_weight 250
# specify the decay for the final state particles

define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

decay t > w+ b
decay t~ > w- b~
decay w+ > l+ vl
decay w- > l- vl~
#decay z > l+ l-
decay h > b b~
# running the actual code
launch
